#include <iostream> 

using namespace std;

int main()

{

double num1 = 0, num2 = 0, sum = 0, quotient = 0;

cout << "Enter two numbers ";
cin >> num1;
cin >> num2;
sum = (num1 + num2);
quotient = (num1 * num2);
cout <<"\n\n";
cout << "The sum of the two numbers is: "<<sum<<".";
cout <<"\n\n";
cout << "The quotient of the two numbers is: "<<quotient<<".";
return 0;
}
