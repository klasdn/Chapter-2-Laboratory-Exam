#include <iostream> 

using namespace std;

int main()

{

double num1 = 0, num2 = 0, difference = 0;

cout << "Enter two numbers to solve the difference ";
cout <<"\n\n";
cin >> num1;
cin >> num2;
difference = (num1 - num2);
cout << "The differnece of the two numbers is: "<<difference<<".";
return 0;
}
